bs_package_flags = -bs-package-name bs-test-monorepo
src_root_dir = /Users/gabrielr.abreu/projects/astrocoders/bs-test-monorepo/packages/app
bsc = /Users/gabrielr.abreu/projects/astrocoders/bs-test-monorepo/node_modules/bs-platform/lib/bsc.exe
bsdep = /Users/gabrielr.abreu/projects/astrocoders/bs-test-monorepo/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101 -warn-error +101
bsc_flags = -bs-suffix -nostdlib -I '/Users/gabrielr.abreu/projects/astrocoders/bs-test-monorepo/node_modules/bs-platform/lib/ocaml' -color always
ppx_flags = 
bs_package_includes = -I /Users/gabrielr.abreu/projects/astrocoders/bs-test-monorepo/node_modules/word-utils/lib/ocaml
bs_package_dev_includes = 
namespace = 
bsb_dir_group = 0
bsc_lib_includes = -I src
rule build_ast_and_module_sets
  command = ${bsc}  ${pp_flags} ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/demo.mlast : build_ast_and_module_sets $src_root_dir/src/demo.ml
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/demo.mlast.d : build_deps src/demo.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/demo.cmj |  $src_root_dir/src/demo.bs.js src/demo.cmi : build_cmj_cmi src/demo.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
build build.ninja : phony  ||  src/demo.mlast.d
